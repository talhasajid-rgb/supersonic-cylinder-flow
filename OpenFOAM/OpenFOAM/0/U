FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;   
        value           uniform (1041.57 0 0);  // It is for Mach 3, for Mach 1.7 is 590;
    }

    outlet
    {
        type            zeroGradient;  
    }

    topfarfield
    {
        type            zeroGradient;  
    }

    bottomfarfield
    {
        type            zeroGradient;
    }

    hot_cylinder
    {
        type            slip;         
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}

