FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}

application     foamRun;
solver          shockFluid;

startFrom       latestTime;
startTime       0;

stopAt          endTime;
endTime         0.01;


deltaT          1e-9;


adjustTimeStep  yes;


maxCo           0.5;        
maxDeltaT       1e-7;       


writeControl    timeStep;
writeInterval   1000;      
writeAtEnd      yes;

purgeWrite      0;

writeFormat     ascii;
writePrecision  8;

runTimeModifiable yes;

// ************************************************************************* //
